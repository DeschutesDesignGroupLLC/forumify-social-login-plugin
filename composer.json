{
    "name": "deschutesdesigngroupllc/forumify-social-login-plugin",
    "description": "A forumify plugin to enable social login.",
    "type": "forumify-plugin",
    "keywords": ["forumify", "social", "login"],
    "license": "OSL-3.0",
    "require": {
        "php": "^8.3",
        "forumify/forumify-platform": "@dev",
        "league/oauth2-google": "^4.0",
        "wohali/oauth2-discord-new": "^1.2",
        "knpuniversity/oauth2-client-bundle": "^2.18"
    },
    "repositories": [
        {
            "type": "path",
            "url": "/Users/jonerickson/PhpstormProjects/forumify-platform"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "DeschutesDesignGroupLLC\\SocialLoginPlugin\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": false,
            "symfony/flex": false
        }
    },
    "require-dev": {
        "laravel/pint": "^1.17"
    },
    "scripts": {
        "lint": "./vendor/bin/pint src"
    },
    "extra": {
        "forumify-plugin-class": "DeschutesDesignGroupLLC\\SocialLoginPlugin\\ForumifySocialLoginPlugin"
    }
}
